<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    
    <title>@ViewBag.Title - AdminLTE 2 | Dashboard</title>

    @*---=== core CSS ===---*@
    @*Tell the browser to be responsive to screen width*@ 
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    @*Bootstrap 3.3.6*@ 
    <link rel="stylesheet" href="~/Areas/AdminPanel/Content/Theme/bootstrap/css/bootstrap.min.css">
    @*Font Awesome*@ 
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.5.0/css/font-awesome.min.css">
    @*Ionicons*@ 
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css">
    @*jvectormap*@ 
    <link rel="stylesheet" href="~/Areas/AdminPanel/Content/Theme/plugins/jvectormap/jquery-jvectormap-1.2.2.css">
    @*Theme style*@ 
    <link href="~/Areas/AdminPanel/Content/Theme/dist/css/AdminLTE.css" rel="stylesheet" />
    @*AdminLTE Skins. Choose a skin from the css/skins folder instead of downloading all of them to reduce the load.*@ 
    <link href="~/Areas/AdminPanel/Content/Theme/dist/css/skins/skin-blue.css" rel="stylesheet" />

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->

    @*---=== bundles-CSS ===---*@
    @*@Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")*@
</head>
<body class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper">

        @*---=== Вехняя панель: Лого, Брэнд и Меню ===---*@
        <header class="main-header">
            @*---=== Logo ===---*@ 
            @Html.Partial("_LogoAndBrand")
            @*---=== Конец Logo ===---*@
            
            @*---=== Вехнее меню ===---*@ 
            @Html.Partial("_HeaderMenu")
            @*---=== Конец Вехнее меню ===---*@
        </header>
        @*---=== Конец Вехняя панель: Лого, Брэнд и Меню ===---*@

        @*---=== Левая панель - LeftSidebar ===---*@ 
        @Html.Partial("_LeftSidebar")
        @*---=== Конец Левая панель - sidebar ===---*@

        @*---=== Облатсть контента ===---*@ 
        <div class="content-wrapper">
            @RenderBody()
        </div>
        @*---=== Конец Облатсть контента ===---*@

        @*---=== Подвал ===---*@
        @Html.Partial("_Footer")
        @*---=== Конец Подвал ===---*@

        @*---=== Правая скрываемая панель настроек ===---*@
        @Html.Partial("_RightControlSidebar")
        @*---=== Конец Правая скрываемая панель настроек ===---*@
    </div>
    @*---=== Конец страницы ===---*@

    <!-- jQuery 2.2.3 -->
    <script src="~/Areas/AdminPanel/Content/Theme/plugins/jQuery/jQuery-2.2.0.min.js"></script>
    <!-- Bootstrap 3.3.6 -->
    <script src="~/Areas/AdminPanel/Content/Theme/bootstrap/js/bootstrap.min.js"></script>
    <!-- FastClick -->
    <script src="~/Areas/AdminPanel/Content/Theme/plugins/fastclick/fastclick.js"></script>
    <!-- AdminLTE App -->
    <script src="~/Areas/AdminPanel/Content/Theme/dist/js/app.min.js"></script>
    <!-- Sparkline -->
    <script src="~/Areas/AdminPanel/Content/Theme/plugins/sparkline/jquery.sparkline.min.js"></script>
    <!-- jvectormap -->
    <script src="~/Areas/AdminPanel/Content/Theme/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js"></script>
    <script src="~/Areas/AdminPanel/Content/Theme/plugins/jvectormap/jquery-jvectormap-world-mill-en.js"></script>
    <!-- SlimScroll 1.3.0 -->
    <script src="~/Areas/AdminPanel/Content/Theme/plugins/slimScroll/jquery.slimscroll.min.js"></script>
    <!-- ChartJS 1.0.1 -->
    @*<script src="~/Areas/AdminPanel/Content/Theme/plugins/chartjs/Chart.min.js"></script>*@
    <!-- AdminLTE dashboard demo (This is only for demo purposes) -->
    @*<script src="~/Areas/AdminPanel/Content/Theme/dist/js/pages/dashboard2.js"></script>*@
    <!-- AdminLTE for demo purposes -->
    @*<script src="~/Areas/AdminPanel/Content/Theme/dist/js/demo.js"></script>*@

    @*---=== bundles-JavaScripts ===---*@
    @*@Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")*@

    @*---=== Секция дополнительных скирптов JavaScripts ===---*@
    @RenderSection("scripts", required: false)
</body>
</html>

